  },
  "homepage": "https://github.com/LloyBit/pet-QRB-hardhat#readme",
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "chai": "^4.5.0",
    "hardhat": "^2.26.3"
  },
  "directories": {
    "test": "test"
  }
